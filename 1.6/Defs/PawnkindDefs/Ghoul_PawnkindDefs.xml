        <initialResistanceRange>15~24</initialResistanceRange>
        <backstoryFiltersOverride>
            <li>
                <categoriesChildhood>
                    <li>FCP_Backstory_Wastelander_Child</li>
                </categoriesChildhood>
                <categoriesAdulthood>
                    <li>FCP_Backstory_Ghoul</li>
                </categoriesAdulthood>
            </li>
        </backstoryFiltersOverride>
    </PawnKindDef>

